{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs", 
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "jsx": "react",
    "lib": ["es5", "es6", "dom", "dom.iterable"],
    "downlevelIteration": true
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules", "public", ".cache"]
}
